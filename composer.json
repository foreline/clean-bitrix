{
  "name": "foreline/clean-bitrix",
  "description": "Implementation of foreline/clean PHP framework for 1C-Bitrix",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Simakin Dima",
      "email": "dima@foreline.ru"
    }
  ],
  "require": {
    "php": ">=8.0",
    "foreline/clean": "~0.5.01",
    "webmozart/assert": "^1.11",
    "symfony/http-foundation": ">6.3",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "phpstan/phpstan": "*",
    "jetbrains/phpstorm-attributes": "*"
  },
  "autoload": {
    "psr-4": {
      "Domain\\": "src/Domain",
      "Infrastructure\\": "src/Infrastructure",
      "Presentation\\": "src/Presentation"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/foreline/clean.git"
    }
  ]
